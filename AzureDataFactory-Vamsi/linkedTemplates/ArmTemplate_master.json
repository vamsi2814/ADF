{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "AzureDataFactory-Vamsi"
		},
		"LS_AzureDB_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_AzureDB'"
		},
		"LS_Blob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_Blob'"
		},
		"LS_TestDB_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_TestDB'"
		},
		"LS_AzureDB_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "vamsisqlserver1.database.windows.net"
		},
		"LS_AzureDB_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "VamsiDB"
		},
		"LS_AzureDB_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "vadlamudi"
		},
		"LS_TestDB_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "vamsisqlserver1.database.windows.net"
		},
		"LS_TestDB_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "@{linkedService().DB_Name}"
		},
		"LS_TestDB_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "vadlamudi"
		},
		"RestService1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dummy.restapiexample.com/api/v1/employees"
		},
		"Event_Trigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/285e89a1-bc34-4c75-b167-9b977b8a723c/resourceGroups/RG-Test/providers/Microsoft.Storage/storageAccounts/adlsvamsiazure"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "AzureDataFactory-Vamsi_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_AzureDB_password": {
						"value": "[parameters('LS_AzureDB_password')]"
					},
					"LS_Blob_connectionString": {
						"value": "[parameters('LS_Blob_connectionString')]"
					},
					"LS_TestDB_password": {
						"value": "[parameters('LS_TestDB_password')]"
					},
					"LS_AzureDB_properties_typeProperties_server": {
						"value": "[parameters('LS_AzureDB_properties_typeProperties_server')]"
					},
					"LS_AzureDB_properties_typeProperties_database": {
						"value": "[parameters('LS_AzureDB_properties_typeProperties_database')]"
					},
					"LS_AzureDB_properties_typeProperties_userName": {
						"value": "[parameters('LS_AzureDB_properties_typeProperties_userName')]"
					},
					"LS_TestDB_properties_typeProperties_server": {
						"value": "[parameters('LS_TestDB_properties_typeProperties_server')]"
					},
					"LS_TestDB_properties_typeProperties_database": {
						"value": "[parameters('LS_TestDB_properties_typeProperties_database')]"
					},
					"LS_TestDB_properties_typeProperties_userName": {
						"value": "[parameters('LS_TestDB_properties_typeProperties_userName')]"
					},
					"RestService1_properties_typeProperties_url": {
						"value": "[parameters('RestService1_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "AzureDataFactory-Vamsi_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'AzureDataFactory-Vamsi_ArmTemplate_0')]"
			]
		},
		{
			"name": "AzureDataFactory-Vamsi_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Event_Trigger_properties_typeProperties_scope": {
						"value": "[parameters('Event_Trigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'AzureDataFactory-Vamsi_ArmTemplate_1')]"
			]
		}
	]
}